{
  "name": "@builder.io/angular",
  "version": "0.1.15-alpha.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4205",
    "_build": "ng build --prod",
    "build": "npm run package",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "package": "ng-packagr -p ng-package.json",
    "develop:package": "chokidar 'src/**/*.html' 'src/**/*.css' 'src/**/*.ts' | ng-packagr",
    "release": "npm run package && cd dist && npm publish",
    "release:patch": "npm run package && npm version patch && cd dist && npm version patch && npm publish",
    "release:minor": "npm run package && npm version minor && cd dist && npm version minor && npm publish",
    "release:prerelease:nightly": "npm run package && npm version prerelease && cd dist && npm version prerelease && npm publish --tag nightly",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "dependencies": {
    "@angular/animations": "^7.0.4",
    "@angular/common": "^7.0.4",
    "@angular/core": "^7.0.4",
    "@angular/http": "^7.0.4",
    "@angular/router": "^7.0.4",
    "@builder.io/sdk": "^1.0.6-alpha.0",
    "core-js": "^2.4.1",
    "lodash-es": "^4.17.11",
    "rxjs": "^6.3.3",
    "tsickle": "^0.33.1",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "^7.0.6",
    "@angular/compiler": "^7.0.4",
    "@angular/compiler-cli": "^7.0.4",
    "@angular/forms": "^7.0.4",
    "@angular/platform-browser": "^7.0.4",
    "@angular/platform-browser-dynamic": "^7.0.4",
    "@angular/platform-server": "^7.0.4",
    "@nguniversal/express-engine": "^7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "@types/express": "^4.11.1",
    "@types/jasmine": "^2.8.11",
    "@types/jasminewd2": "^2.0.6",
    "@types/lodash-es": "^4.17.1",
    "@types/node": "^10.12.8",
    "chokidar-cli": "^1.2.1",
    "codelyzer": "^4.5.0",
    "express": "^4.16.4",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.0",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.4.0",
    "protractor": "^5.4.1",
    "ts-loader": "^5.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  }
}
