{
  "name": "@builder.io/cli",
  "version": "1.0.1",
  "description": "Official Builder.io CLI",
  "author": "Steve Sewell <steve@builder.io>",
  "keywords": [
    "builder.io",
    "cli"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "bin": "./dist/cli.js",
  "files": [
    "dist/**/*",
    "!dist/**/*.test.js"
  ],
  "scripts": {
    "dev": "ts-node ./src/cli.ts",
    "clean": "rimraf ./dist/ ./exec/",
    "build": "npm run clean && tsc",
    "test": "npm run build && ava --verbose",
    "bundle": "npm run build && pkg . --out-dir ./exec/"
  },
  "devDependencies": {
    "ava": "^2.2.0",
    "pkg": "^4.4.0",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "commander": "^2.20.0"
  }
}
